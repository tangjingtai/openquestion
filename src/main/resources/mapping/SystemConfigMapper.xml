<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jt.openquestion.mapper.KnowledgePointMapper">
    <select id="getSystemConfig" resultType="com.jt.openquestion.entity.SystemConfigRaw">
        SELECT
          `ConfigID`,
          `ConfigName`,
          `ConfigValue`,
          `Description`
        FROM `systemconfig`
    </select>
</mapper>